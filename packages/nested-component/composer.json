{
    "name": "test-org/nested-component",
    "description": "Deep nested component with the most critical dependency confusion vulnerabilities",
    "type": "library",
    "license": "MIT",
    "version": "2.0.0",
    "repositories": [
        {
            "type": "path",
            "url": "./deep-missing-critical-path"
        },
        {
            "type": "path",
            "url": "../../../external-missing-lib"
        },
        {
            "type": "vcs",
            "url": "https://github.com/ultra-missing-org/ultra-missing-repo.git"
        },
        {
            "type": "vcs", 
            "url": "git@github.com:deep-missing-org/deep-missing-package.git"
        },
        {
            "type": "composer",
            "url": "https://deep-private-registry.corp.com/"
        },
        {
            "type": "artifact",
            "url": "./artifacts/"
        }
    ],
    "require": {
        "php": "^8.1",
        "psr/log": "^3.0",
        "ultra-critical-missing-package": "^5.0",
        "deep-nested-critical-vuln": "dev-master",
        "enterprise-critical-lib": "^4.0",
        "missing-core-component": "^2.5",
        "critical-infrastructure-lib": "^3.0",
        "deep-missing-dependency": "dev-feature-v2",
        "ultra-missing-tool": "^1.8",
        "critical-nested-missing-final": "^2.0",
        "high-risk-missing-package": "^3.5",
        "corporate-internal-missing": "dev-main",
        "fiber/promises": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "ultra-critical-dev-missing": "^2.0",
        "deep-test-lib-critical": "dev-experimental-v2",
        "missing-dev-framework": "^3.5",
        "critical-debug-tools": "^1.5",
        "enterprise-dev-missing": "^4.0",
        "deep-nested-test-missing": "^2.5",
        "ultra-dev-helpers": "dev-bleeding-edge",
        "critical-mock-framework": "^3.0",
        "high-risk-dev-tools": "^2.0",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "TestOrg\\NestedComponent\\": "src/",
            "TestOrg\\DeepMissing\\": "./deep-missing-critical-path/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TestOrg\\NestedComponent\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit --coverage-html coverage/",
        "analyze": "phpstan analyse src/ --level=8",
        "cs-fix": "php-cs-fixer fix src/",
        "security-check": "security-checker security:check"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "platform": {
            "php": "8.1.0"
        }
    },
    "homepage": "https://github.com/ultra-missing-critical-org/nested-component-missing",
    "support": {
        "source": "https://github.com/deep-missing-support-org/nested-component-repo",
        "issues": "https://github.com/critical-missing-org/nested-issues-repo",
        "docs": "https://github.com/missing-docs-org/component-docs"
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/missing-sponsor-org"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-main": "2.0.x-dev"
        },
        "laravel": {
            "providers": [
                "TestOrg\\NestedComponent\\ServiceProvider"
            ]
        }
    },
    "conflict": {
        "conflicting-missing-package": "<1.0"
    },
    "suggest": {
        "suggested-missing-package": "For enhanced functionality (missing from registry)",
        "optional-missing-lib": "Optional missing library for extended features"
    }
}
